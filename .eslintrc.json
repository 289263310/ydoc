{
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "indent": ["off", 2],
        "comma-dangle": ["error", "never"],
        "no-console": ["off"]
        
    }
}