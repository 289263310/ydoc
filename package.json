{
  "name": "ydoc",
  "version": "3.2.5",
  "description": "Yummy Doc 文档生成工具",
  "keywords": [
    "doc",
    "document"
  ],
  "homepage": "https://ydoc.ymfe.org/",
  "main": "src/index.js",
  "bin": {
    "ydoc": "bin/ydoc"
  },
  "author": {
    "name": "edwon.lin",
    "email": "edwon.lim@gmail.com",
    "url": "https://ymfe.org/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:YMFE/ydoc.git"
  },
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bash-color": "0.0.4",
    "cheerio": "^0.20.0",
    "fs-extra": "^5.0.0",
    "markdown-it": "^8.4.0",
    "nunjucks": "^3.0.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "stylus": "^0.54.5",
    "underscore": "^1.8.3",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "ghooks": "^2.0.0",
    "validate-commit-msg": "^2.12.2"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "test",
        "chore",
        "refactor",
        "opti"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "请输入message信息!",
      "helpMessage": "Commit message 格式错误, \n请查看规范: http://wiki.corp.qunar.com/pages/viewpage.action?pageId=159698767"
    }
  }
}
