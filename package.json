{
  "name": "ydoc",
  "version": "3.2.1",
  "description": "Yummy Doc 文档生成工具",
  "keywords": [
    "doc",
    "document"
  ],
  "homepage": "http://ymfe.tech/ydoc",
  "main": "src/ydoc.js",
  "bin": {
    "ydoc": "bin/ydoc"
  },
  "author": {
    "name": "edwon.lin",
    "email": "edwon.lim@gmail.com",
    "url": "http://ymfe.tech"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:YMFE/ydoc.git"
  },
  "engines": {
    "node": ">=4"
  },
  "license": "MIT",
  "dependencies": {
    "art-template": "^3.0.3",
    "atropa-jsformatter": "^0.1.2",
    "colors": "^1.1.2",
    "comment-parser": "^0.4.0",
    "cpr": "^1.1.1",
    "glob": "^7.0.3",
    "globby": "^6.1.0",
    "html-entities": "^1.2.1",
    "json5": "^0.5.0",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "optimist": "^0.6.1",
    "prismjs": "^1.6.0",
    "shelljs": "^0.7.8",
    "through2": "^2.0.1",
    "watch": "^0.17.1"
  },
  "devDependencies": {
    "ghooks": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^2.0.0",
    "validate-commit-msg": "^2.12.2"
  },
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "test",
        "chore",
        "refactor",
        "opti"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "请输入message信息!",
      "helpMessage": "Commit message 格式错误, \n请查看规范: http://wiki.corp.qunar.com/pages/viewpage.action?pageId=159698767"
    }
  }
}
